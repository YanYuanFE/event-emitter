{
  "name": "@antv/event-emitter",
  "version": "0.1.0",
  "description": "event emitter for antvis.",
  "license": "MIT",
  "main": "lib/index.js",
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:antvis/event-emitter.git"
  },
  "scripts": {
    "build": "rimraf lib && tsc",
    "test": "jest"
  },
  "devDependencies": {
    "@types/jest": "^24.0.18",
    "jest": "^24.9.0",
    "jest-electron": "^0.1.7",
    "rimraf": "^2.6.2",
    "ts-jest": "^24.1.0",
    "typescript": "^3.6.4"
  },
  "jest": {
    "preset": "ts-jest",
    "runner": "jest-electron/runner",
    "testEnvironment": "jest-electron/environment",
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/**/*.ts",
      "!**/node_modules/**"
    ],
    "testRegex": "/__tests__/.*spec\\.ts$"
  }
}
